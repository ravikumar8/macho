{
	"name": "webloper/macho",
    "description": "Macho, macho man. I want to be a macho man",
    "keywords": ["phpunit", "webloper", "macho"],
    "homepage": "https://github.com/ravikumar8/macho",
    "license": "MIT",
    "authors" : [
        {
            "name": "Ravi Kumar",
            "email": "webloper@gmail.com",
            "role": "Developer"
        }
    ],
    "support" : {
        "email" : "webloper@gmail.com",
        "source" : "https://github.com/ravikumar8/macho",
        "issues" : "https://github.com/ravikumar8/macho/issues",
        "wiki" : "https://github.com/ravikumar8/macho/wiki"
    },
    "require" : {
        "php" : ">=5.4"
    },
    "require-dev":	{
		"phpunit/phpunit": "~4.0",
        "phploc/phploc" : "2.1.4",
        "pdepend/pdepend" : "2.1.0",
        "phpmd/phpmd" : "2.2.3",
        "sebastian/phpcpd" : "2.0.2",
        "theseer/phpdox" : "0.8.1.1",
        "satooshi/php-coveralls" : "dev-master",
        "phpspec/phpspec": "~2.1",
        "mikey179/vfsStream": "~1.4.0",
        "squizlabs/php_codesniffer": "~1.5"
	},
	"autoload":	{
		"psr-4": {
			"Webloper\\Macho\\": "src/"
		}
	},
    "autoload-dev" : {
        "psr-4" : {
            "Webloper\\Tests\\" : [
                "tests/"
            ]
        }
    },
    "config" : {
        "vendor-dir" : "vendor"
    },
    "type" : "library"
}
